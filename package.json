    "debug": "SET SLS_DEBUG=* && node --inspect %AppData%/npm/node_modules/serverless/bin/serverless offline",
    "deploy": "npm test && sls deploy",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "serverless-bundle test"
  },
