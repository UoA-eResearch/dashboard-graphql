{
  "name": "dashboard-graphql",
  "version": "1.0.0",
  "description": "dashboard graphql server",
  "main": "handler.js",
  "dependencies": {
    "apollo-server-lambda": "^2.13.1",
    "graphql": "^15.0.0",
    "graphql-depth-limit": "^1.1.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "serverless-bundle": "^1.6.0",
    "serverless-offline": "^6.1.7"
  },
  "scripts": {
    "start": "sls offline",
    "build": "sls webpack",
